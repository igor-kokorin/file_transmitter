  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch Reader",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "runtimeExecutable": "ts-node",
      "args": [ "${workspaceFolder}\\src\\reader\\index.ts" ]
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch Writer",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "runtimeExecutable": "ts-node",
      "args": [ "${workspaceFolder}\\src\\writer\\index.ts" ]
    },
  ]
}